{
  "name": "@graphql-mocks/network-pretender",
  "version": "0.0.0",
  "author": "Chad Carbert",
  "description": "Mock using graphql-mocks with pretender.js",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "pretest": "tsc --noEmit && yarn lint",
    "test": "TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha -r ts-node/register \"./test/**/*.test.ts\" --exclude \"./test/acceptance/dummy-react-app/node_modules/**/*.*\"",
    "clean": "rimraf ./dist",
    "copy-pjson": "node scripts/copy-scrubbed-pjson",
    "build": "yarn clean && rollup -c rollup.config.js && yarn copy-pjson",
    "watch": "rollup --watch -c rollup-watch.config.js"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "pretender": "^3.4.7"
  },
  "peerDependencies": {
    "pretender": "^3.4.7"
  }
}
