{
  "name": "graphql-mocks",
  "version": "0.8.3",
  "author": "Chad Carbert",
  "description": "Tools for setting up graphql test resolvers",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "pretest": "tsc --noEmit && yarn lint",
    "test": "TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha -r ts-node/register \"./test/**/*.test.ts\"",
    "clean": "rimraf ./dist",
    "copy-pjson": "node scripts/copy-scrubbed-pjson",
    "copy-readme": "cp README.md dist/README.md",
    "build": "yarn clean && rollup -c rollup.config.js && yarn copy-pjson && yarn copy-readme",
    "watch": "rollup --watch -c rollup-watch.config.js"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "optionalDependencies": {
    "sinon": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "miragejs": "^0.1.40",
    "sinon": "^9.0.0"
  }
}