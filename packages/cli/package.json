{
  "name": "gqlmocks",
  "version": "0.5.5",
  "bin": {
    "gqlmocks": "./bin/run"
  },
  "bugs": "https://github.com/graphql-mocks/graphql-mocks/issues",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@graphql-mocks/falso": "^0.7.4",
    "@graphql-mocks/network-express": "^0.4.4",
    "@ngneat/falso": "^8.0.2",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.36",
    "@oclif/plugin-version": "^2.2.36",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "cli-ux": "^6.0.9",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "debug": "^4.4.3",
    "express": "^5.1.0",
    "git-root-dir": "^1.0.2",
    "graphiql-middleware": "^0.0.5",
    "graphql": "^16.12.0",
    "graphql-import-node": "^0.0.5",
    "graphql-mocks": "^0.11.4",
    "handlebars": "^4.7.7",
    "pkg-dir": "^5",
    "ts-node": "^10.5.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@oclif/test": "^3.0.0",
    "@types/chai": "^5.2.3",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.7",
    "@types/git-root-dir": "^1.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.1.0",
    "@types/sinon": "^21.0.0",
    "chai": "^6.2.1",
    "globby": "^16.0.0",
    "mocha": "^11.0.0",
    "oclif": "4.22.50",
    "sinon": "^21.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/graphql-mocks/graphql-mocks",
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "repositoryPrefix": "<%- repo %>/blob/main/packages/cli/<%- commandPath %>",
    "topicSeparator": " ",
    "commands": "./lib/commands",
    "bin": "gqlmocks",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-version",
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-mocks/graphql-mocks"
  },
  "keywords": [
    "oclif",
    "graphql-mocks",
    "cli",
    "gqlmocks",
    "graphql"
  ],
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && oclif readme && pnpm build",
    "test": "NODE_ENV=development mocha \"test/**/*.test.ts\"",
    "build": "tsc && pnpm copy-blueprints && oclif readme && pnpm copy-readme && oclif manifest",
    "test-build": "mocha \"test/**/*.test.ts\"",
    "copy-blueprints": "mkdir -p lib/blueprints && cp -r src/blueprints lib",
    "copy-readme": "cp README.md lib/README.md"
  },
  "types": "lib/index.d.ts"
}
