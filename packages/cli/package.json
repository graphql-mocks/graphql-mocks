{
  "name": "gqlmocks",
  "version": "0.0.0",
  "author": "Chad Carbert @chadian",
  "bin": {
    "gqlmocks": "./bin/run"
  },
  "bugs": "https://github.com/chadian/gqlmocks/issues",
  "dependencies": {
    "@graphql-mocks/faker": "^0.2.0",
    "@graphql-mocks/network-express": "^0.1.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "axios": "^0.23.0",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.3",
    "express": "^4.17.1",
    "graphiql-middleware": "^0.0.4",
    "graphql": "^15.6.1",
    "graphql-mocks": "0.8.1",
    "ts-node": "^10.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "sinon": "^12.0.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/chadian/gqlmocks",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "bin": "gqlmocks",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "chadian/gqlmocks",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
